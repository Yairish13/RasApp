Missing = missings.filter((missing)=>{ \r\n            return missing.id === isFilled[0].missingId ;\r\n        })\r\n        chosenMissing[0] && setFirstOption({id:chosenMissing[0].id,name: chosenMissing[0].missingName})\r\n    }\r\n    useEffect(()=>{\r\n        isFilled.length > 0 && getFirstOption();\r\n    },[])\r\n    return (\r\n        <NohRowContainer>\r\n            <FormName>{soldier.firstName}</FormName>\r\n            <FormName>{soldier.lastName}</FormName>\r\n            <Select ref={selectCurrent} onChange={()=>handle()}>\r\n                {\r\n                    firstOption !== null \r\n                    ?\r\n                    <option value={firstOption.id}>{firstOption.name}</option>\r\n                    :\r\n                    <option></option>\r\n                }\r\n                {\r\n                    missings.map((missing,i)=>{\r\n                        return <option key={i} value={missing.id}>{missing.missingName}</option>\r\n                    })\r\n                }\r\n            </Select>\r\n        </NohRowContainer>\r\n    )\r\n}\r\n\r\nexport default NohehutRow\r\n","C:\\Users\\yairi\\Desktop\\Yair\\Tasks\\RasApp\\client\\src\\components\\SoldierRow.js",["228"],"import React from 'react'\r\nimport styled from 'styled-components';\r\nimport { useHistory } from 'react-router-dom' ;\r\nimport { SoldierRowContainer } from '../styledComponents/soldierspage';\r\nimport { FormName } from '../styledComponents/global';\r\nfunction SoldierRow({soldier}) {\r\n    const history = useHistory();\r\n    return (\r\n        <SoldierRowContainer onClick={()=>history.push(`/soldier/${soldier.id}`)}>\r\n            <FormName>{soldier.id}</FormName>\r\n            <FormName>{soldier.firstName}</FormName>\r\n            <FormName>{soldier.lastName}</FormName>\r\n        </SoldierRowContainer>\r\n            \r\n    \r\n    )\r\n}\r\n\r\nexport default SoldierRow\r\n","C:\\Users\\yairi\\Desktop\\Yair\\Tasks\\RasApp\\client\\src\\pages\\Profil